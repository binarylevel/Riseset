source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.0'
use_frameworks!

target :Riseset do
  pod 'RxSwift',    '~> 2.0'
  pod 'RxCocoa',    '~> 2.0'
  pod 'RxBlocking', '~> 2.0'
  pod 'Reachability'
  pod 'SVProgressHUD'
  pod 'PureLayout'
  pod 'Mixpanel'
  pod 'Fabric'
  pod 'Crashlytics'
  pod 'Alamofire', '~> 3.0'
  pod 'RealmSwift'
end

target :RisesetTests do
  link_with 'RisesetTests'
  pod 'Specta', '~> 1.0'
  pod 'Expecta', '~> 1.0'
  pod 'Quick', '~> 0.8.0'
  pod 'Nimble', '3.0.0'
end